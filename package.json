{
  "name": "weather-bar-app",
  "version": "1.1.0",
  "author": {
    "name": "Peter Schmalfeldt",
    "email": "me@peterschmalfeldt.com",
    "url": "http://peterschmalfeldt.com"
  },
  "description": "Weather Bar is a Weather Application that lives in your Menu Bar giving you beautiful access to real-time weather conditions and a generous 15-day forecast.",
  "keywords": [
    "Electron",
    "Vue",
    "Weather",
    "Menubar",
    "Forecast",
    "MacOS",
    "Windows",
    "Linux"
  ],
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manifestinteractive/weather-bar-app.git"
  },
  "bugs": {
    "url": "https://github.com/manifestinteractive/weather-bar-app/issues"
  },
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "cross-env NODE_ENV=development node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js",
    "unit:coverage": "open test/unit/coverage/lcov-report/index.html",
    "web": "npm run build:web && node node_modules/http-server/bin/http-server ./dist/web -o -s -p8081 -c-1 --cors"
  },
  "build": {
    "productName": "Weather Bar",
    "copyright": "Copyright Â© 2022 Peter Schmalfeldt",
    "appId": "org.simulatedgreg.electron-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "background": "build/img/background.png",
      "icon": "build/icons/icon.icns",
      "title": "Weather Bar v1.1.0",
      "iconTextSize": 16,
      "iconSize": 120,
      "contents": [
        {
          "x": 425,
          "y": 235,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 120,
          "y": 235,
          "type": "file"
        }
      ]
    },
    "mac": {
      "category": "public.app-category.weather",
      "icon": "build/icons/icon.icns",
      "files": [
        "!dist/electron/static/linux",
        "!dist/electron/static/win"
      ]
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "files": [
        "!dist/electron/static/linux",
        "!dist/electron/static/mac"
      ]
    },
    "linux": {
      "icon": "build/icons/icon.png",
      "files": [
        "!dist/electron/static/mac",
        "!dist/electron/static/win"
      ]
    }
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "axios": "^0.27.2",
    "bugsnag": "^2.1.3",
    "d3-scale": "^4.0.2",
    "debounce": "^1.2.1",
    "dotenv-webpack": "^8.0.0",
    "electron-updater": "^5.0.5",
    "feels": "^3.0.0",
    "lodash": "^4.17.21",
    "logger-electron": "^1.7.4",
    "md5": "^2.3.0",
    "menubar": "^9.2.1",
    "moment-timezone": "^0.5.34",
    "nedb": "^1.8.0",
    "node-machine-id": "^1.1.12",
    "semver": "^7.3.7",
    "suncalc": "^1.9.0",
    "superagent": "^8.0.0",
    "tz-lookup": "^6.1.25",
    "vue": "^3.2.37",
    "vue-awesome-swiper": "^5.0.1",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^9.1.10",
    "vue-moment": "^4.1.0",
    "vue-router": "^4.1.2",
    "vue-toastr": "^2.1.2",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^3.1.1",
    "chai": "^4.3.6",
    "chalk": "^5.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "del": "^6.1.1",
    "devtron": "^1.4.0",
    "electron": "^19.0.8",
    "electron-builder": "^23.1.0",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.20.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^4.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "inject-loader": "^4.0.1",
    "karma": "^6.4.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^2.2.0",
    "karma-electron": "^7.2.0",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "^0.0.34",
    "karma-webpack": "^5.0.0",
    "mocha": "^10.0.0",
    "multispinner": "^0.2.1",
    "node-loader": "^2.0.0",
    "node-sass": "^7.0.1",
    "require-dir": "^1.2.0",
    "sass-loader": "^13.0.2",
    "spectron": "^19.0.0",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.7",
    "webpack": "^5.73.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-merge": "^5.8.0"
  }
}
